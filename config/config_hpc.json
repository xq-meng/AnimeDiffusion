{
    "model": {
        "train": {
            "batch_size": 128,
            "epochs": 100,
            "ema_decay": 0.999,
            "learning_rate": 5e-4,
            "save_epochs": 20
        },
        "test": {
            "batch_size": 1
        },
        "diffusion": {
            "time_step": 1000,
            "betas": {
                "linear_start": 1e-6,
                "linear_end": 1e-2
            },
            "unet": {
                "channel_in": 3,
                "channel_out": 2,
                "channel_mult": [1, 2, 2, 2]
            }
        }
    },
    "directories": {
        "dataset": "./dataset",
        "status_save": "/output/status",
        "status_load": "",
        "test_result": "/output/test"
    }
}
